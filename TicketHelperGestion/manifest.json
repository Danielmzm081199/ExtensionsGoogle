{
  "manifest_version": 3,
  "name": "Ticket Helper Licenciamiento",
  "description": "Ahorra tiempo con tu portapapeles online para el m√≥dulo Gestion",
  "version": "1.0",
  "permissions": [
      "clipboardWrite",
      "clipboardRead"
  ],
  "action": {
      "default_popup": "popup.html",
      "default_icon": {
          "16": "image.png",
          "32": "image.png",
          "48": "image.png",
          "128": "image.png"
      }
  },
  "icons": {
      "16": "image.png",
      "32": "image.png",
      "48": "image.png",
      "128": "image.png"
  },
  "host_permissions": [
      "http://*/*",
      "https://*/*"
  ],
  "content_security_policy": {
      "script-src": "'self'",
      "object-src": "'self'"
  }
}
